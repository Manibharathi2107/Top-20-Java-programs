import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    String a=sc.nextLine();
	    String b=" ";
	    char ch;
	    for(int i=0;i<a.length();i++){
	        ch=a.charAt(i);
	        b=ch+b;
	    }
		System.out.println(b);
	}
}
