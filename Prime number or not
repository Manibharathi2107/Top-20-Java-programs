import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int n=sc.nextInt();
	    boolean t=true;
	    for(int i=2;i<n;i++){
	        if(n%i==0){
	            t=false;
	            break;
	        }
	    }
		System.out.println(t);
	}
}
